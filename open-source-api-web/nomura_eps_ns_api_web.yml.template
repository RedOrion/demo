# rename this file to nomura::eps::ns::api::web.yml and put a ':' after 'name' if
# you want to use YAML like in old versions of Catalyst
name:   Nomura::EPS::NS::API::Web

# Change the DATABASE,USERNAME and PASSWORD to match the database set up for the developer
DBD:
    dsn:            'DBI:mysql:host=localhost;database=DATABASE'
    username:       USERNAME
    password:       PASSWORD
    dbi_attr:       {AutoCommit: 1}
